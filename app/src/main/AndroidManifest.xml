<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.kolacbb.androiddemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AndroidHeros.MeasureViewActivity" />
        <activity android:name=".AndroidHeros.CustomViewListActivity" />
        <activity android:name=".AndroidHeros.CustomView.CustomViewExpandActivity" />
        <activity android:name=".AndroidHeros.CustomView.CustomViewMergerActivity" />
        <activity android:name=".AndroidHeros.CustomView.CustomViewNewActivity" />
        <activity android:name=".AndroidArtExplore.MessagePrincipal.HandlerActivity"></activity>
    </application>

</manifest>